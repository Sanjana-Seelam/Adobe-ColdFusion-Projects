<cfcomponent>
    <cffunction name="getAllProfiles" access="public" returntype="array" output="false">
        <cfset var profiles = []>

        <!--- Simulate fetching data from a database --->
        <cfset profiles = [
            {id=1, name="Alice Johnson", email="alice.johnson@example.com", age=30},
            {id=2, name="Bob Smith", email="bob.smith@example.com", age=25},
            {id=3, name="Charlie Davis", email="charlie.davis@example.com", age=35}
        ]>

        <cfreturn profiles>
    </cffunction>
